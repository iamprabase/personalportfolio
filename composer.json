{
  "name": "jessicajacoson/author-post",
  "description": "A minimalist cms for for an author's website, featuring articles about her novel and allowing registered members to comment on articles.",
  "type": "project",
  "require": {
    "cocur/slugify": "^4.6",
    "php-di/php-di": "^7.0",
    "slim/csrf": "^1.5",
    "slim/flash": "^0.4.0",
    "slim/psr7": "^1.7",
    "slim/slim": "^4.0",
    "slim/twig-view": "^3.4",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
